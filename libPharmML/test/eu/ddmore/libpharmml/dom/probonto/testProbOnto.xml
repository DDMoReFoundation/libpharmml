<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mml:PharmML xmlns:msteps="http://www.pharmml.org/pharmml/0.7/ModellingSteps" xmlns:td="http://www.pharmml.org/pharmml/0.7/TrialDesign" xmlns:uncert="http://www.uncertml.org/3.0" xmlns:ds="http://www.pharmml.org/pharmml/0.7/Dataset" xmlns:mdef="http://www.pharmml.org/pharmml/0.7/ModelDefinition" xmlns:ct="http://www.pharmml.org/pharmml/0.7/CommonTypes" xmlns:math="http://www.pharmml.org/pharmml/0.7/Maths" xmlns:mml="http://www.pharmml.org/pharmml/0.7/PharmML" writtenVersion="0.7">
    <ct:Name>Test ProbOnto model</ct:Name>
    <mdef:ModelDefinition>
        <mdef:ParameterModel blkId="pm1">
            <mdef:IndividualParameter>
                <mdef:Distribution>
                    <mdef:ProbOnto name="Normal1">
                        <mdef:Parameter name="mean">
                            <ct:Assign>
                                <ct:Real>0.0</ct:Real>
                            </ct:Assign>
                        </mdef:Parameter>
                        <mdef:Parameter name="var">
                            <ct:Assign>
                                <ct:Real>1.0</ct:Real>
                            </ct:Assign>
                        </mdef:Parameter>
                        <mdef:LowerTruncationBound>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="lowerSymbol"/>
                            </ct:Assign>
                        </mdef:LowerTruncationBound>
                        <mdef:UpperTruncationBound>
                            <ct:Assign>
                                <ct:SymbRef symbIdRef="upperSymbol"/>
                            </ct:Assign>
                        </mdef:UpperTruncationBound>
                        <mdef:MixtureComponent name="Beta">
                            <mdef:Parameter name="inverseScaleMatrix">
                                <ct:Assign>
                                    <ct:plusInf/>
                                </ct:Assign>
                            </mdef:Parameter>
                        </mdef:MixtureComponent>
                    </mdef:ProbOnto>
                </mdef:Distribution>
            </mdef:IndividualParameter>
        </mdef:ParameterModel>
    </mdef:ModelDefinition>
</mml:PharmML>